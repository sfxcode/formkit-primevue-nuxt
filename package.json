{
  "name": "@sfxcode/formkit-primevue-nuxt",
  "version": "1.5.5",
  "description": "FormKit PrimeVue nuxt module",
  "homepage": "https://github.com/sfxcode/formkit-primevue-nuxt",
  "repository": "https://github.com/sfxcode/formkit-primevue-nuxt",
  "license": "MIT",
  "type": "module",
  "build": {
    "failOnWarn": false
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "dev:start": "node playground/.output/server/index.mjs",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --patch --release && npm publish --access=public && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@formkit/nuxt": "^1.6.9",
    "@nuxtjs/i18n": "^10.1.0",
    "@primevue/nuxt-module": "^4.4.1",
    "@sfxcode/formkit-primevue": "^3.0.11"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.5",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/kit": "^4.1.3",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.1.3",
    "@nuxt/test-utils": "^3.19.2",
    "@primeuix/themes": "^1.2.5",
    "@primevue/forms": "^4.4.1",
    "@types/node": "^24.7.2",
    "changelogen": "^0.6.2",
    "eslint": "^9.37.0",
    "nuxt": "^4.1.3",
    "typescript": "5.9.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1"
  },
  "packageManager": "pnpm@10.18.2+sha512.9fb969fa749b3ade6035e0f109f0b8a60b5d08a1a87fdf72e337da90dcc93336e2280ca4e44f2358a649b83c17959e9993e777c2080879f3801e6f0d999ad3dd"
}
