{
  "name": "@sfxcode/formkit-primevue-nuxt",
  "version": "1.3.8",
  "description": "FormKit PrimeVue nuxt module",
  "homepage": "https://github.com/sfxcode/formkit-primevue-nuxt",
  "repository": "https://github.com/sfxcode/formkit-primevue-nuxt",
  "license": "MIT",
  "type": "module",
  "build": {
    "failOnWarn": false
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --patch --release && npm publish --access=public && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@formkit/nuxt": "^1.6.9",
    "@nuxt/kit": "^3.17.5",
    "@nuxtjs/i18n": "^9.5.5",
    "@primevue/nuxt-module": "^4.3.5",
    "@sfxcode/formkit-primevue": "^2.9.6"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.5.0",
    "@nuxt/eslint-config": "^1.4.1",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.17.5",
    "@nuxt/test-utils": "^3.19.1",
    "@primeuix/themes": "^1.1.2",
    "@primevue/forms": "^4.3.5",
    "@types/node": "^24.0.3",
    "changelogen": "^0.6.1",
    "eslint": "^9.29.0",
    "nuxt": "^3.17.5",
    "typescript": "5.8.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.10"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}
