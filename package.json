{
  "name": "@sfxcode/formkit-primevue-nuxt",
  "version": "1.2.13",
  "description": "FormKit PrimeVue nuxt module",
  "homepage": "https://github.com/sfxcode/formkit-primevue-nuxt",
  "repository": "https://github.com/sfxcode/formkit-primevue-nuxt",
  "license": "MIT",
  "type": "module",
  "build": {
    "failOnWarn": false
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --patch --release && npm publish --access=public && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@formkit/nuxt": "^1.6.9",
    "@nuxt/kit": "^3.15.4",
    "@nuxtjs/i18n": "^9.2.1",
    "@primevue/nuxt-module": "^4.2.5",
    "@sfxcode/formkit-primevue": "^2.7.6"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.1.0",
    "@nuxt/eslint-config": "^1.1.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.4",
    "@nuxt/test-utils": "^3.17.0",
    "@primevue/themes": "^4.2.5",
    "@types/node": "latest",
    "changelogen": "^0.5.7",
    "eslint": "^9.21.0",
    "nuxt": "^3.15.4",
    "typescript": "5.6.3",
    "vitest": "^3.0.6",
    "vue-tsc": "^2.2.4"
  },
  "packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af"
}
